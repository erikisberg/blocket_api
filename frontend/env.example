# Anthropic API Key for AI analysis
# Get your key from: https://console.anthropic.com/
ANTHROPIC_API_KEY=your_api_key_here

# Base URL for the application (used for server-side API calls)
NEXT_PUBLIC_BASE_URL=http://localhost:3000

# Database Configuration
NEON_DATABASE_URL=postgresql://username:password@host:port/database

# SMS Service (46elk)
FORTYSIXELK_API_KEY=your_46elk_api_key_here
FORTYSIXELK_SENDER=BlocketAlert

# Cron Job Security
CRON_SECRET=your_random_secret_here

# Optional: Customize AI analysis settings
AI_ANALYSIS_MODEL=claude-opus-4-1-20250805
AI_ANALYSIS_TEMPERATURE=0.1
